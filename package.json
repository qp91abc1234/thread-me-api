    "class-validator": "^0.14.1",
    "dotenv": "^17.2.3",
    "faiss-node": "^0.5.1",
    "langchain": "^0.3.9",
    "multer": "1.4.5-lts.1",
    "nest-winston": "^1.9.7",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
